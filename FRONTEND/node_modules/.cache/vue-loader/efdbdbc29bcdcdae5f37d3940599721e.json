r\n       \n      >\n      <Workdone_Input @message=\"addTodo\"/>\n       <v-list class=\"todo_list\">\n        <Workdone_Todo v-for=\"(todo,index) in list\" \n        @delete=\"deleteTodo\"\n        @done=\"doneTodo\"\n        :title=\"todo.message\" \n        :id=\"todo.id\" \n        :done=\"todo.done\" \n        :key=\"index\"></Workdone_Todo>\n      </v-list>\n     \n      </v-col>\n      \n     <v-col\n       cols=\"12\"\n       sm=\"6\"\n       \n      >\n       <Current_Activity_Input @message=\"addTodo_one\"/>\n       <v-list class=\"todo_list\">\n        <Current_Activity_Todo v-for=\"(todo1,index) in list1\" \n        @delete=\"deleteTodo_one\"\n        @done=\"doneTodo_one\"\n        :title=\"todo1.message1\" \n        :id=\"todo1.id\" \n        :done=\"todo1.done\" \n        :key=\"index\"></Current_Activity_Todo>\n      </v-list>\n      </v-col>\n     \n      <v-col\n       cols=\"12\"\n       sm=\"6\"\n       \n      icon\n      >\n      <Blocking_Issues_Input @message=\"addTodo_two\"/>\n       <v-list class=\"todo_list\">\n        <Blocking_Issues_Todo v-for=\"(todo,index) in list\" \n        @delete=\"deleteTodo_two\"\n        @done=\"doneTodo_two\"\n        :title=\"todo2.message\" \n        :id=\"todo2.id\" \n        :done=\"todo2.done\" \n        :key=\"index\"></Blocking_Issues_Todo>\n      </v-list>\n     \n      </v-col>\n        <v-col\n       cols=\"12\"\n       sm=\"6\"\n       \n      icon\n      >\n      <Way_Forward_Input @sendMessagemessage=\"addTodo\"/>\n       <v-list class=\"todo_list\">\n        <Way_Forward_Todo v-for=\"(todo,index) in list\" \n        @delete=\"deleteTodo\"\n        @done=\"doneTodo\"\n        :title=\"todo.message\" \n        :id=\"todo.id\" \n        :done=\"todo.done\" \n        :key=\"index\"></Way_Forward_Todo>\n      </v-list>\n     \n      </v-col>\n      <br>\n      <v-col\n       cols=\"12\"\n       sm=\"6\"\n      icon\n      >\n        <v-combobox small v-model=\"status\" :items=\"items1\" chips clearab