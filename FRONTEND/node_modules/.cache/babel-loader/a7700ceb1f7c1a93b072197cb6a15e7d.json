ick=\"modal = false\"\n                                        >\n                                          Cancel\n                                        </v-btn>\n                                        <v-btn\n                                          text\n                                          color=\"primary\"\n                                          @click=\"$refs.dialog.save(date)\"\n                                        >\n                                          OK\n                                        </v-btn>\n                                      </v-date-picker>\n                                    </v-dialog>\n                                  </v-layout>\n                                </v-container>\n                              </v-card-text>\n                              <v-card-actions>\n                                <v-spacer></v-spacer>\n                                <v-btn\n                                  class=\"captosmall\"\n                                  color=\"primary\"\n                                  text\n                                  outlined\n                                  x-small\n                                  rounded\n                                  @click=\"dialog = false\"\n                                >\n                                  Schedule\n                                </v-btn>\n                                <v-btn\n                                  class=\"captosmall\"\n                                  color=\"blue darken-1\"\n                                  text\n                                  outlined\n                                  x-small\n                                  @click=\"dialog = false\"\n                                >\n                                  Save\n                                </v-btn>\n                              </v-card-actions>\n                            </v-card>\n                          </v-dialog>\n                        </v-row>\n                      </template>\n                    </div>\n                    <br />\n                    <div class=\"my-2\">\n                      <v-btn\n                        x-small\n                        outlined\n                        rounded\n                        color=\"error\"\n                        @click=\"showAlert\"\n                        class=\"captosmall\"\n                      >\n                        Reject\n                      </v-btn>\n                    </div>\n                  </div>\n                </v-col>\n              </v-layout>\n            </v-card>\n          </v-container>\n        </v-card>\n      </v-card>\n    </v-container>\n  </v-app>\n</template>\n<script>\nimport gql from \"graphql-tag\";\n\nlet QUERY = gql`\n  query {\n    countNewAppointments\n  }\n`;\n\nexport default {\n  methods: {\n    showAlert() {\n      // Use sweetalert2\n      // this.$swal('Hello Vue world!!!');\n      this.$swal\n        .fire({\n          title: \"Are you sure?\",\n          text: \"You won't be able to revert this!\",\n          icon: \"warning\",\n          showCancelButton: true,\n          confirmButtonColor: \"#3085d6\",\n          cancelButtonColor: \"#d33\",\n          confirmButtonText: \"Yes, delete it!\",\n        })\n        .then((result) => {\n          if (result.isConfirmed) {\n            this.$swal.fire(\n              \"Deleted!\",\n              \"Your file has been deleted.\",\n              \"success\"\n            );\n          }\n        });\n    },\n    retreivingNewAppoint() {},\n  },\n  data: () => ({\n    dialog: false,\n    time: null,\n    menu2: false,\n    modal2: false,\n    newAppoint: null,\n  }),\n  apollo: {\n    newAppoint: {\n      query: QUERY,\n    },\n    update: (data) => data.QUERY,\n  },\n};\n</script>\n<style scoped>\n.weka {\n  justify-content: center;\n  align-items: center;\n}\n.captosmall {\n  text-transform: unset !important;\n}\n</style>\n"]}]}